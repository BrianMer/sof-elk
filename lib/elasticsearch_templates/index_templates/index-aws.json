{
  "index_patterns": [
    "aws-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-final"
  ],
  "template": {
    "mappings": {
      "runtime": {
        "source_geo.asnstr": {
          "type": "keyword",
          "script": {
            "source": "if (doc.containsKey('source_geo.asn') && doc['source_geo.asn'].size() != 0 && doc.containsKey('source_geo.as_org.keyword') && doc['source_geo.as_org.keyword'].size() != 0) { String m = 'ASN' + doc['source_geo.asn'].value + ': ' + doc['source_geo.as_org.keyword']; emit(m); }"
          }
        }
      },
      "properties": {
        "raw_request_parameters": {
          "type": "keyword"
        },
        "raw_response_parameters": {
          "type": "keyword"
        },
        "raw_session_context": {
          "type": "keyword"
        },
        "type": {
          "type": "keyword"
        },
        "version": {
          "type": "integer"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
