{
  "index_patterns": [
    "microsoft365-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-guid",
    "mappings-port",
    "mappings-final"
  ],
  "template": {
    "mappings": {
      "dynamic_templates": [
        {
          "sid_fields": {
            "mapping": {
              "type": "keyword"
            },
            "match": "*_sid"
          }
        }
      ],
      "runtime": {
        "source_geo.asnstr": {
          "type": "keyword",
          "script": {
            "source": "if (doc.containsKey('source_geo.asn') && doc['source_geo.asn'].size() != 0 && doc.containsKey('source_geo.as_org.keyword') && doc['source_geo.as_org.keyword'].size() != 0) { String m = 'ASN' + doc['source_geo.asn'].value + ': ' + doc['source_geo.as_org.keyword']; emit(m); }"
          }
        },
        "client_geo.asnstr": {
          "type": "keyword",
          "script": {
            "source": "if (doc.containsKey('client_geo.asn') && doc['client_geo.asn'].size() != 0 && doc.containsKey('client_geo.as_org.keyword') && doc['client_geo.as_org.keyword'].size() != 0) { String m = 'ASN' + doc['client_geo.asn'].value + ': ' + doc['client_geo.as_org.keyword']; emit(m); }"
          }
        }
      },
      "properties": {
        "actor": {
          "type": "object"
        },
        "folder": {
          "properties": {
            "Id": {
              "type": "keyword"
            }
          },
          "type": "object"
        },
        "folders": {
          "type": "object"
        },
        "is_valid": {
          "type": "boolean"
        },
        "modified_properties": {
          "type": "object"
        },
        "operation": {
          "type": "keyword"
        },
        "operation_properties": {
          "type": "object"
        },
        "operations": {
          "type": "keyword"
        },
        "ps_show_computer_name": {
          "type": "boolean"
        },
        "result_status": {
          "type": "keyword"
        },
        "target": {
          "type": "object"
        },
        "type": {
          "type": "keyword"
        },
        "user_type": {
          "type": "integer"
        },
        "version": {
          "type": "integer"
        },
        "workload": {
          "type": "keyword"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
