{
  "index_patterns": [
    "httpdlog-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash"
  ],
  "template": {
    "mappings": {
      "runtime": {
        "source_geo.asnstr": {
          "type": "keyword",
          "script": {
            "source": "String m; if ( doc['source_geo.asn'].size() == 0 || doc['source_geo.as_org.keyword'].size() == 0 ) { m = 'ASN: Not Available' } else { m = ('ASN' + doc['source_geo.asn'].value + ': ' + doc['source_geo.as_org.keyword'].value) } emit(m);"
          }
        }
      },
      "properties": {
        "httpversion": {
          "type": "keyword"
        },
        "ident": {
          "type": "keyword"
        },
        "response_code": {
          "type": "short"
        },
        "request_method": {
          "type": "keyword"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
