{
  "index_patterns": [
    "httpdlog-*"
  ],
  "composed_of": [
    "settings-common",
    "mappings-corecommon",
    "mappings-string",
    "mappings-ip_address",
    "mappings-geohash",
    "mappings-final"
  ],
  "template": {
    "mappings": {
      "runtime": {
        "source_geo.asnstr": {
          "type": "keyword",
          "script": {
            "source": "String m; if ( doc.containsKey('source_geo.asn') && doc.containsKey('source_geo.as_org.keyword') ) { m = ('ASN' + doc['source_geo.asn'].value + ': ' + doc['source_geo.as_org.keyword'].value); emit(m) }"
          }
        }
      },
      "properties": {
        "httpversion": {
          "type": "keyword"
        },
        "ident": {
          "type": "keyword"
        },
        "response_code": {
          "type": "short"
        },
        "request_method": {
          "type": "keyword"
        }
      }
    }
  },
  "_meta": {
    "provider": "SOF-ELK"
  }
}
